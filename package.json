{
  "name": "flowdsl",
  "displayName": "flowdsl",
  "description": "Node-based game engine DSL support ",
  "version": "0.0.1",
  "repository": { "type": "git", "url": "https://github.com/Mahsery/flowdsl" },

  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "flowdsl",
      "aliases": ["FlowDSL", "flowdsl"],
      "extensions": [".flow"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "flowdsl",
      "scopeName": "source.flowdsl",
      "path": "./syntaxes/flowdsl.tmLanguage.json"
    }]
  }
}
